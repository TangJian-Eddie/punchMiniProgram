.title {
  font-size: 28rpx;
  margin: 20rpx 0;
}
.input {
  background-color: rgba(230, 230, 230, 0.6);
  padding: 10rpx 20rpx;
  border-radius: 14rpx;
  margin: 20rpx 0;
}
.iconList {
  display: grid;
  grid-template-columns: repeat(5, 20%);
  grid-row-gap: 30rpx;
  justify-items: center;
  margin: 20rpx 0;
}